	rm -f ../lib/gcc_$(__dbtreegrid_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_dbtreegrid-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so
	rm -f ../sample/sample

test_for_selected_wxbuild: 
	@$(WX_CONFIG) $(WX_CONFIG_FLAGS)

ifeq ($(SHARED),0)
../lib/gcc_$(__dbtreegrid_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_dbtreegrid-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a: make_dir_dbtreegrid_lib  $(DBTREEGRID_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(DBTREEGRID_LIB_OBJECTS)
	$(RANLIB) $@
endif

ifeq ($(SHARED),0)
install_dbtreegrid_lib: $(__dbtreegrid_lib___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__dbtreegrid_lib__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_dbtreegrid-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a $(DESTDIR)$(prefix)/lib
endif

ifeq ($(SHARED),0)
uninstall_dbtreegrid_lib: 
	rm -f $(DESTDIR)$(prefix)/lib/libwxcode_$(WX_PORT)_dbtreegrid-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).a
endif

make_dir_dbtreegrid_lib: 
	@mkdir -p ../lib/gcc_$(__dbtreegrid_lib__DIRNAME_SHARED_SUFFIX)

ifeq ($(SHARED),0)
install_dbtreegrid_lib_headers: 
	$(INSTALL) -d $(DESTDIR)$(prefix)
	for f in $(DBTREEGRID_LIB_HEADERS); do \
	if test ! -d $(DESTDIR)$(prefix)/`dirname $$f` ; then \
	$(INSTALL) -d $(DESTDIR)$(prefix)/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 ../$$f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(SHARED),0)
uninstall_dbtreegrid_lib_headers: 
	for f in $(DBTREEGRID_LIB_HEADERS); do \
	rm -f $(DESTDIR)$(prefix)/$$f; \
	done
endif

ifeq ($(SHARED),1)
../lib/gcc_$(__dbtreegrid_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_dbtreegrid-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so: make_dir_dbtreegrid_dll  $(DBTREEGRID_DLL_OBJECTS)
	$(CXX) $(LINK_DLL_FLAGS) -fPIC -o $@ $(DBTREEGRID_DLL_OBJECTS)  $(VAR_1) -L../lib $(LDFLAGS)  `$(WX_CONFIG) $(WX_CONFIG_FLAGS) --libs adv,core,base`
endif

ifeq ($(SHARED),1)
install_dbtreegrid_dll: $(__dbtreegrid_dll___depname)
	$(INSTALL) -d $(DESTDIR)$(prefix)/lib
	$(INSTALL) -m 644 ../lib/gcc_$(__dbtreegrid_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_dbtreegrid-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
	install -c ../lib/gcc_$(__dbtreegrid_dll__DIRNAME_SHARED_SUFFIX)/libwxcode_$(WX_PORT)_dbtreegrid-$(WX_VERSION_MAJOR).$(WX_VERSION_MINOR).so $(DESTDIR)$(prefix)/lib
endif

